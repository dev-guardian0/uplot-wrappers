{
  "name": "uplot-wrappers",
  "version": "1.0.0",
  "scripts": {
    "build": "concurrently 'npm run buildUMD' 'npm run buildCommonJS' 'npm run buildProd'",
    "buildUMD": "webpack --env=libraryTarget=umd",
    "buildCommonJS": "webpack --env=libraryTarget=commonjs",
    "buildProd": "webpack --env=libraryTarget=umd --env=mode=production"
  },
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "babel-loader": "^8.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.0"
  }
}
