{
    "name": "uplot-wrappers",
    "version": "1.0.0",
    "description": "React and Vue.js wrappers for uPlot",
    "author": "Sergey Kalinichev <kalinichev.so.0@gmail.com>",
    "license": "MIT",
    "homepage": "https://github.com/user/project",
    "repository": {
        "type": "git",
        "url": "https://github.com/user/project.git"
    },
    "bugs": {
        "url": "https://github.com/user/project/issues"
    },
    "engines": {
        "node": ">=8.10"
    },
    "keywords": [
        "Charts",
        "uPlot",
        "React",
        "Vue",
        "Wrapper"
    ],
    "workspaces": {
        "packages": ["common", "react", "vue"],
        "nohoist": ["**/@types/**"]
    },
    "private": true,
    "scripts": {
        "build": "concurrently 'npm run buildReact' 'npm run buildVue'",
        "buildVue": "webpack --env=framework=vue",
        "buildReact": "webpack --env=framework=react",
        "buildProd": "concurrently 'npm run buildVue -- --env=mode=production' 'npm run buildReact -- --env=mode=production'"
    },
    "dependencies": {
    },
    "devDependencies": {
        "@babel/core": "^7.13.10",
        "@babel/preset-env": "^7.13.10",
        "@babel/preset-react": "^7.12.13",
        "@vue/babel-preset-jsx": "^1.2.4",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "babel-loader": "^8.2.2",
        "concurrently": "^6.0.0",
        "copy-webpack-plugin": "4",
        "eslint": "^7.22.0",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-vue": "^7.7.0",
        "eslint-webpack-plugin": "^2.5.2",
        "ts-loader": "^8.0.18",
        "typescript": "^4.2.3",
        "webpack": "^4.46.0",
        "webpack-cli": "^4.5.0"
    }
}
